/* global Vue */
document.addEventListener("DOMContentLoaded", function(event) { 
  var app = new Vue({
    el: '#app',
    data: {
      message: 'fdsagdsagas',
      games: []
    },
    mounted: function() {
        $.ajax({
          url: "<%= ENV['API_URL'] %>" + "/games/?fields=name,popularity&order=popularity:desc&limit=10",
          type: "GET",
          headers: {
            "user-key": "<%= ENV['API_KEY']%>",
            "Accept": "application/json"
          }, 
          success: function(response) {
            this.games = response;
          }.bind(this)
        });
      },
    methods: {

    },
    computed: {

    }
  });
});